{
  "name": "DeepWiki AutoRedirect",
  "description": "github automatically redirects to deepwiki Shortcut can be changed at [chrome://extensions/shortcuts]",
  "version": "1.0.9",
  "author": "h_ypi",
  "homepage_url": "https://github.com/dada994a/deepwiki_autoredirect",
  "manifest_version": 3,

  "host_permissions": [ "https://github.com/*", "https://deepwiki.com/*" ],
  "permissions": ["tabs"],

	"icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },

  "background": {
    "service_worker": "background.js"
  },

  "commands": {
    "redirect": {
      "suggested_key": { "default": "Ctrl+G", "mac": "Command+G" },
      "description": "Back and forth between Github and DeepWiki"
    }
  }
}
